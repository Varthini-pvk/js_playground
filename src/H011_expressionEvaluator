// Write a function evaluate(a, b, operator)
// operator can be "+", "-", "*", "/", "%"
console.log(evaluate(10, 5, "+")); // 15
console.log(evaluate(10, 5, "*")); // 50


function evaluate(a, b, operator){
    switch (operator)
    {
    case "+": return a+b;
    break;
    case "-": return a-b;
    break;
    case "*": return a*b;
    break;
    case "/": return a/b;
    break;
    case "%": return a%b;
    break;
    default: return null;
    }

}
